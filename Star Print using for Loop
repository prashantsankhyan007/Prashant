import java.util.*;
import java.lang.*;
import java.io.*;
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		for (int i=0;i<=5;i++){
			for(int j=0;j<=5;j++){
				if(i+j<=4){
					System.out.print("  ");
				}
				else{
					System.out.print(" *");
				}
			
			}
			System.out.println("");
		}
	}
}
